{
    "label": "Guitar",

    "nodes": [{
        "id":    "tone-synth",
        "type":  "nodes/tone-synth.js",
        "label": "Tone Synth",
        "data": {
            "sources": [{
                "type": "noise",
                "data": {
                    "type": "white",
                    "mix": 0.5,
                    "pan": -0.8
                }
            }, {
                "type": "tone",
                "data": {
                    "type": "sine",
                    "detune": 0,
                    "mix": 0.25,
                    "pan": 0.8
                }
            }, {
                "type": "tone",
                "data": {
                    "type": "square",
                    "detune": -1208,
                    "mix": 0.125,
                    "pan": 0.2
                }
            }],

            "gainEnvelope": {
                "attack": [
                    [0, "step", 0],
                    [0.01, "linear", 1],
                    [0.4, "exponential", 0.125],
                    [1, "target", 0.5, 0.4]
                ],

                "release": [
                    [0, "target", 0, 0.1]
                ]
            },

            "frequencyEnvelope": {
                "attack": [
                    [0,    "step",   0],
                    [0.06, "linear", 1000],
                    [5,    "exponential", 200]
                ],

                "release": [
                    [0,  "target", 0, 0.02]
                ]
            },

            "type": "lowpass",
            "frequency": 1,
            "Q": 5,
            "output": 0.1
        }
    }, {
        "id":    "flanger",
        "type":  "nodes/flanger.js",
        "label": "Flanger",
        "data": {
            "delay":     0.024,
            "frequency": 0.333333,
            "depth":     0.004,
            "feedback":  0.9,
            "dry":       1,
            "wet":       1
        }
    }, {
        "id":    "output",
        "type":  "output",
        "label": "Output",
        "data": {}
    }],

    "connections": [
        { "source": "tone-synth", "target": "flanger" },
        { "source": "flanger", "target": "output" }
    ],

    "controls": [{
        "source": {
            "device": "keyboard",
            "type":   "notes"
        },

        "target": "tone-synth",

        "data": {
            "transform": "linear",
            "type":   "all",
            "min":    0,
            "max":    1
        }
    }, {
        "source": {
            "device": "midi",
            "channel": 1,
            "type":    "all"
        },

        "target": "tone-synth",

        "data": {
            "transform": "linear",
            "type":   "all"
        }
    }],

    "sequences": [{
        "id": "test",
        "events": [
            [0, "note", 49, 0.5, 1]
        ]
    }],

    "events": [
        [0, "sequence", "test", "synth", 2]
    ]
}
